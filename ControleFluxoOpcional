  public static void main(String[] args) {
	for (int i = 100; i <= 200; i++) {
	  if (i % 2 == 0 && i % 3 == 0) {
		System.out
		    .println(i
		        + " é divisível por 2 e divisível por 3 entao é por 6");
	  } else if (i % 2 == 0) {
		System.out.println(i + " é divisível por 2");
	  } else if (i % 3 == 0) {
		System.out.println(i + " é divisível por 3");
	  }
	}
  }
}
