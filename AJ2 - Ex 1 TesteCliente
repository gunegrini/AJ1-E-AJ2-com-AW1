public class TestaCliente {

    public static void main(String[] args) {
        Cliente c = new Cliente();
        c.nome = "Globalcode";
        c.cpf = "123154";
        System.out.println("  ");
        System.out.println("Nome: " + c.nome + "\tCPF: " + c.cpf);
        System.out.println("------------------------------------");
    }
}
